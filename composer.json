{
	"name": "anlutro/l4-core",
	"description": "Core L4 functionality.",
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Lutro",
			"email": "anlutro@gmail.com"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/anlutro-pull-requests/laravel"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"nesbot/carbon": "*",
		"anlutro/l4-repository": "0.5.*",
		"anlutro/l4-controller": "0.1.*",
		"anlutro/l4-testing": "0.1.*"
	},
	"require-dev": {
		"laravel/laravel": "dev-fix/get-bootstrap-file",
		"mockery/mockery": "dev-master@dev",
		"anlutro/menu": "0.1.*"
	},
	"suggest": {
		"anlutro/menu": "Menu builder."
	},
	"autoload": {
		"psr-4": {
			"c\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"minimum-stability": "stable",
	"bin": ["bin/scaff"]
}
