{
	"name": "anlutro/l4-core",
	"description": "Core L4 functionality.",
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Lutro",
			"email": "anlutro@gmail.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"laravel/framework": "~4.2",
		"anlutro/l4-repository": "0.7.*",
		"anlutro/l4-controller": "0.3.*",
		"anlutro/l4-testing": "0.2.*"
	},
	"require-dev": {
		"laravel/laravel": "dev-master",
		"mockery/mockery": "~0.9",
		"anlutro/menu": "~0.2"
	},
	"suggest": {
		"anlutro/menu": "Menu builder."
	},
	"autoload": {
		"psr-4": {
			"anlutro\\Core\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"anlutro\\Core\\Tests\\": "tests/src/"
		}
	},
	"config": {
		"preferred-install": "dist"	
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
